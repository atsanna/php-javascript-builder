{
    "name": "netzmacht/php-javascript-builder",
    "description": "PHP Javascript builder library",
    "type": "library",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "David Molineus",
            "email": "david.molineus@netzmacht.de"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpcq/all-tasks": "~1.2",
        "phpspec/phpspec": "~4.0 || ~5.0",
        "symfony/event-dispatcher": "~2.3 || ~3.0 || ~4.0"
    },
    "suggest": {
        "symfony/event-dispatcher": "Install the symfony event dispatcher to use the event dispatching encoder."
    },
    "autoload": {
        "psr-4": {
            "Netzmacht\\JavascriptBuilder\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.1.x-dev"
        }
    }
}
